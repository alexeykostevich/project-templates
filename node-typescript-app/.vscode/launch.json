{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "type": "pwa-node",
      "program": "${workspaceFolder}/src/index.ts",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "sourceMaps": true
    }
  ]
}
